<html>
<head>
<script src="http://code.jquery.com/jquery-latest.js">
</script>
<script>
$(document).ready(function () 
{
	$("#but1").click(function () 
	{
		
		$("#div1").fadeIn("slow");
		$("#div1").fadeIn(2000);
		$("#div2").fadeOut("hide");
		$("#div2").fadeIn(2000);
		$("#div2").fadeIn("slow");
		$("#div2").fadeIn(2000);
		$("#div3").fadeOut("hide");
		$("#div3").fadeIn(4000);
		$("#div3").fadeIn("slow");
		$("#div3").fadeIn(2000);
		$("#div4").fadeOut("hide");
		$("#div4").fadeIn(6000);
		$("#div4").fadeIn("slow");
		$("#div4").fadeIn(2000);
		$("#div5").fadeOut("hide");
		$("#div5").fadeIn(8000);
		$("#div5").fadeIn("slow");
		$("#div5").fadeIn(2000);
		$("#div1").fadeOut("hide");
		$("#div1").fadeOut(12000);
		$("#div1").fadeOut("slow");
		$("#div1").fadeOut(2000);
		$("#div2").fadeOut("hide");
		$("#div2").fadeOut(14000)
		$("#div2").fadeOut("slow");
		$("#div2").fadeOut(2000);
		$("#div3").fadeOut("hide");
		$("#div3").fadeOut(16000);
		$("#div3").fadeOut("slow");
		$("#div3").fadeOut(2000);
		$("#div4").fadeOut("hide");
		$("#div4").fadeOut(18000);
		$("#div4").fadeOut("slow");
		$("#div4").fadeOut(2000);
		$("#div5").fadeOut("hide");
		$("#div5").fadeOut(20000);
		$("#div5").fadeOut("slow");
		$("#div5").fadeOut(2000);
	});
});
</script>
</head>
<body>
<p>Нажать на кнопку.</p>
<button id="but1">Нажми</button>

<br><br>

<div id="div1" style="width:40px;height:40px;display:none;background-color:blue; float:left;"></div>
<div id="div2" style="width:40px;height:40px;display:none;background-color:red;float:left;"></div>
<div id="div3" style="width:40px;height:40px;display:none;background-color:green;float:left;"></div>
<div id="div4" style="width:40px;height:40px;display:none;background-color:pink;float:left;"></div>
<div id="div5" style="width:40px;height:40px;display:none;background-color:red;float:left;"></div>

</body>
</html>
